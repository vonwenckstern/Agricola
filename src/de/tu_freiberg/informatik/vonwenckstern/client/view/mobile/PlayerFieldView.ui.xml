			<g:PushButton text="finish special events and continue feeding your family" ui:field="btnFeedingFamily" visible="false"/>
		</g:HorizontalPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<a:AcquisitionCardRenderer position="4"/>
